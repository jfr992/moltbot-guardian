# Moltbot Security Dashboard - Environment Variables
# Copy to .env and fill in your values

# Flask secret key (generate with: python -c "import secrets; print(secrets.token_hex(32))")
SECRET_KEY=

# Clawdbot API token (from ~/.clawdbot/clawdbot.json)
CLAWDBOT_API_TOKEN=

# Grafana admin password (optional, defaults to 'guardian' for local dev)
GF_ADMIN_PASSWORD=

# OpenTelemetry endpoint (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
